		  	<a href="javascript:;">new user <i class="fa fa-plus-circle"></i></a>
	  	</div>
	    <div data-role="listview" class="listview-outlook">

	    <% users.forEach(function (user) { %>
	      <a href="javascript:;" class="list user-link" data-user="<%= user.id %>">
